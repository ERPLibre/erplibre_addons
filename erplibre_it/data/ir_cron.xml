<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">
    <record model="ir.cron" id="ir_cron_it_check">
        <field name="name">ERPLibre IT check up cron</field>
        <field name="active" eval="False" />
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
        <field name="model_id" ref="model_it_workspace" />
        <field name="state">code</field>
        <field name="code">model.action_check_all()</field>
        <field name="nextcall" eval="(datetime.now() + timedelta(minutes=1)).strftime('%Y-%m-%d %H:%M:00')" />
    </record>
</odoo>
