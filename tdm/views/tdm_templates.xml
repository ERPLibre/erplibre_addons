    <template id="tdm.portal_my_tdm_entrevue_stages" name="My Tdm Entrevue Stages">
        <t t-call="portal.portal_layout">
            <t t-set="breadcrumbs_searchbar" t-value="True" />
            <t t-call="portal.portal_searchbar">
                <t t-set="title">Tdm Entrevue Stages</t>
            </t>
            <t t-if="not tdm_entrevue_stages">
                <div class="alert alert-warning mt8" role="alert">There are no tdm_entrevue_stages.</div>
            </t>
            <t t-if="tdm_entrevue_stages" t-call="portal.portal_table">
                <tbody>
                    <tr t-foreach="tdm_entrevue_stages" t-as="tdm_entrevue_stage">
                        <td>
                            <a t-attf-href="/my/tdm_entrevue_stage/#{tdm_entrevue_stage.id}?{{ keep_query() }}">
                                <span t-field="tdm_entrevue_stage.name" />
                            </a>
                        </td>
                        <td class="text-right" />
                    </tr>
                </tbody>
            </t>
        </t>
    </template>

