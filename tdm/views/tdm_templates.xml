<odoo>
    <template id="tdm.portal_my_tdm_entrevue" name="My Tdm Entrevue">
        <t t-call="portal.portal_layout">
            <t t-set="o_portal_fullwidth_alert">
                <t t-call="portal.portal_back_in_edit_mode">
                    <t
                        t-set="backend_url"
                        t-value="'/web#return_label=Website&amp;model=tdm.entrevue&amp;id=%s&amp;view_type=form' % (tdm_entrevue.id)"
                    />
                </t>
            </t>
            <t t-call="portal.portal_record_layout">
                <t t-set="card_header">
                    <h5 class="mb-0">
                        <small class="text-muted">Tdm Entrevue -</small>
                        <span t-field="tdm_entrevue.name" />
                    </h5>
                </t>
                <t t-set="card_body">
                    <div class="row" />
                </t>
            </t>
        </t>
    </template>

