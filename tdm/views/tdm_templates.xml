    <template id="tdm.portal_my_home" name="Portal My Home: Tdm entries" inherit_id="portal.portal_my_home">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-if="tdm_offre_emploi_count" t-call="portal.portal_docs_entry">
                <t t-set="title">(T/E)Toutes les offres d'emplois</t>
                <t t-set="url" t-value="'/my/tdm_offre_emplois'" />
                <t t-set="count" t-value="tdm_offre_emploi_count" />
            </t>
            <t t-if="tdm_offre_emploi_count" t-call="portal.portal_docs_entry">
                <t t-set="title">(E)Tes offre d'emplois</t>
                <t t-set="url" t-value="'/my/tdm_offre_emplois'" />
                <t t-set="count" t-value="tdm_offre_emploi_count" />
            </t>
            <t t-if="tdm_entrevue_count" t-call="portal.portal_docs_entry">
                <t t-set="title">(T/E)Tes entrevues</t>
                <t t-set="url" t-value="'/my/tdm_entrevues'" />
                <t t-set="count" t-value="tdm_entrevue_count" />
            </t>
            <t t-if="tdm_offre_emploi_applique_count" t-call="portal.portal_docs_entry">
                <t t-set="title">(T)Application</t>
                <t t-set="url" t-value="'/my/tdm_offre_emploi_appliques'" />
                <t t-set="count" t-value="tdm_offre_emploi_applique_count" />
            </t>
            <t t-if="tdm_offre_emploi_applique_count" t-call="portal.portal_docs_entry">
                <t t-set="title">(E)Candidature reçu</t>
                <t t-set="url" t-value="'/my/tdm_offre_emploi_appliques'" />
                <t t-set="count" t-value="tdm_offre_emploi_applique_count" />
            </t>
        </xpath>
    </template>

    <template id="tdm.portal_layout" name="Portal layout: Tdm menu entry" inherit_id="portal.portal_breadcrumbs">
        <xpath expr="//ol[hasclass('o_portal_submenu')]" position="inside">
            <li
                t-if="page_name == 'tdm_entrevue' or tdm_entrevue"
                t-attf-class="breadcrumb-item #{'active ' if not tdm_entrevue else ''}"
            >
                <a t-if="tdm_entrevue" t-attf-href="/my/tdm_entrevues?{{ keep_query() }}">Tdm Entrevues</a>
                <t t-else="">Tdm Entrevues</t>
            </li>
            <li t-if="tdm_entrevue" class="breadcrumb-item active">
                <t t-esc="tdm_entrevue.name" />
            </li>
            <li
                t-if="page_name == 'tdm_entrevue_stage' or tdm_entrevue_stage"
                t-attf-class="breadcrumb-item #{'active ' if not tdm_entrevue_stage else ''}"
            >
                <a t-if="tdm_entrevue_stage" t-attf-href="/my/tdm_entrevue_stages?{{ keep_query() }}">
                    Tdm Entrevue Stages
                </a>
                <t t-else="">Tdm Entrevue Stages</t>
            </li>
            <li t-if="tdm_entrevue_stage" class="breadcrumb-item active">
                <t t-esc="tdm_entrevue_stage.name" />
            </li>
            <li
                t-if="page_name == 'tdm_offre_emploi' or tdm_offre_emploi"
                t-attf-class="breadcrumb-item #{'active ' if not tdm_offre_emploi else ''}"
            >
                <a t-if="tdm_offre_emploi" t-attf-href="/my/tdm_offre_emplois?{{ keep_query() }}">Tdm Offre Emplois</a>
                <t t-else="">Tdm Offre Emplois</t>
            </li>
            <li t-if="tdm_offre_emploi" class="breadcrumb-item active">
                <t t-esc="tdm_offre_emploi.name" />
            </li>
            <li
                t-if="page_name == 'tdm_offre_emploi_applique' or tdm_offre_emploi_applique"
                t-attf-class="breadcrumb-item #{'active ' if not tdm_offre_emploi_applique else ''}"
            >
                <a t-if="tdm_offre_emploi_applique" t-attf-href="/my/tdm_offre_emploi_appliques?{{ keep_query() }}">
                    Tdm Offre Emploi Appliques
                </a>
                <t t-else="">Tdm Offre Emploi Appliques</t>
            </li>
            <li t-if="tdm_offre_emploi_applique" class="breadcrumb-item active">
                <t t-esc="tdm_offre_emploi_applique.name" />
            </li>
            <li
                t-if="page_name == 'tdm_offre_emploi_applique_stage' or tdm_offre_emploi_applique_stage"
                t-attf-class="breadcrumb-item #{'active ' if not tdm_offre_emploi_applique_stage else ''}"
            >
                <a
                    t-if="tdm_offre_emploi_applique_stage"
                    t-attf-href="/my/tdm_offre_emploi_applique_stages?{{ keep_query() }}"
                >
                    Tdm Offre Emploi Applique Stages
                </a>
                <t t-else="">Tdm Offre Emploi Applique Stages</t>
            </li>
            <li t-if="tdm_offre_emploi_applique_stage" class="breadcrumb-item active">
                <t t-esc="tdm_offre_emploi_applique_stage.name" />
            </li>
            <li
                t-if="page_name == 'tdm_offre_emploi_stage' or tdm_offre_emploi_stage"
                t-attf-class="breadcrumb-item #{'active ' if not tdm_offre_emploi_stage else ''}"
            >
                <a t-if="tdm_offre_emploi_stage" t-attf-href="/my/tdm_offre_emploi_stages?{{ keep_query() }}">
                    Tdm Offre Emploi Stages
                </a>
                <t t-else="">Tdm Offre Emploi Stages</t>
            </li>
            <li t-if="tdm_offre_emploi_stage" class="breadcrumb-item active">
                <t t-esc="tdm_offre_emploi_stage.name" />
            </li>
        </xpath>
    </template>
</odoo>
